{
  "name": "znck/transformer",
  "description": "A better API with Eloquent.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "transformer",
    "repositories",
    "eloquent",
    "API"
  ],
  "autoload": {
    "psr-4": {
      "Znck\\Transformers\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Znck\\Tests\\Transformers\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Rahul Kadyan",
      "email": "hi@znck.me"
    }
  ],
  "prefer-stable": true,
  "require": {
    "illuminate/support": "^5.3",
    "illuminate/database": "^5.3",
    "illuminate/console": "^5.3",
    "league/fractal": "^0.14"
  },
  "require-dev": {
    "phpunit/php-code-coverage": "^4.0",
    "friendsofphp/php-cs-fixer": "^1.12",
    "satooshi/php-coveralls": "^1.0",
    "phpunit/phpunit": "^5.5",
    "graham-campbell/testbench": "^3.2",
    "mockery/mockery": "^0.9.5"
  },
  "scripts": {
    "test": "phpunit"
  }
}
